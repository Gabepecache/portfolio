<div class="showbox">
	<div class="panel panel-default">
			<%= image_tag @pin.image.url %> <br/>
			<p>
				<%= @pin.description %><br/>
			</p>
			<% if @pin.user == current_user %>
				<%= link_to 'Edit', edit_pin_path(@pin) %> 
			<% end %>
			<%= link_to 'Back', pins_path, class: "btn btn-primary"  %>
	</div>
</div>

